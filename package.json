{
  "name": "speakandspell",
  "author": "Andrea Callea",
  "description": "Speak & Spell in Elm :)",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "elm-i18next-gen": "^1.1.0",
    "elm-review": "^2.8.2",
    "elm-test": "^0.19.1-revision10",
    "elm-tooling": "^1.10.0",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.0",
    "standard": "^17.0.0",
    "standard-markdown": "^7.1.0",
    "stylelint": "^14.15.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "tailwindcss": "^3.2.4",
    "vite": "^3.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-elm": "^2.7.2",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-webfont-dl": "^3.4.1"
  },
  "scripts": {
    "postinstall": "elm-tooling install",
    "dev": "vite",
    "prebuild": "elm-tooling install && npm run localegen",
    "build": "vite build",
    "preview": "vite preview",
    "standard": "standard --fix src/**/*.js",
    "markdown": "standard-markdown --fix src/**/*.md",
    "stylelint": "stylelint --fix 'src/**/*.(css|scss|sass)'",
    "review": "elm-review --fix-all",
    "test": "elm-test-rs",
    "localegen": "elm-i18next-gen --source public/translations/translations.en.json --target src/elm --overwrite"
  },
  "standard": {
    "ignore": [
      "public",
      "dist",
      "tmp.js"
    ]
  }
}
